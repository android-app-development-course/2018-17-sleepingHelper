<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.sleephelper">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Fragment.routineactivity" />
        <activity android:name=".beforesleepactivity" />
        <activity android:name=".daojishioneactivity" />
        <activity android:name=".daojishitwoactivity" />
        <activity android:name=".daojishithreeactivity" />
        <activity android:name=".routinetwoactivity" />
        <activity android:name=".clockactivity" />
        <activity android:name=".recordsleep"></activity>
        <receiver
            android:name=".OneShotAlarm"
            android:process=":remote" />
        <receiver
            android:name=".RepeatingAlarm"
            android:process=":remote" />

    </application>

</manifest>